<div style="float: right; margin-right: 20px" <p><a href="/songs">Dashboard</a> | <a href="/sessions/logout" data-method="delete">Logout</a></div><br>

Others who added <%= @song.artist %> - <%= @song.title %><br>
<br>

<table>
	<thead>
		<tr>
			<th>Name</th>
			<th>Times Added</th>
		</tr>
	</thead>
	<tbody>
		<% @users.uniq.each do |u| %>
			<tr>
				<td><a href="/users/<%= u.id %>"><%= u.full_name %></a></td>
				<td><%= u.adds.where(song_id: params[:id]).count %></td>
			</tr>
		<% end %>
	</tbody>
</table><br>